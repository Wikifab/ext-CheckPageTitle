{
	"name": "Check Page Title",
	"author": "Solene",
	"version": "1.0.0",
	"url": "https://www.mediawiki.org/wiki/Extension:CheckPageTitle",
	"descriptionmsg": "Check Page Title",
	"license-name": "GPL-2.0+",
	"type": "parserhook",
	"ResourceModules": {
